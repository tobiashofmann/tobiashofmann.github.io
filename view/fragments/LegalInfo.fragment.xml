<core:FragmentDefinition 
	xmlns="sap.m" 
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:l="sap.ui.layout"
	>
	<ActionSheet 
		xmlns="sap.m" 
		xmlns:core="sap.ui.core" 
		showCancelButton="true" 
		placement="Bottom" >
		<Button text="{i18n>legaldisclosure}" class="sapUiSizeCompact" icon="sap-icon://role" press="navToLegalDisclosure"/>
		<Button text="{i18n>license}" class="sapUiSizeCompact" icon="sap-icon://co" press="navToLicense"/>
	</ActionSheet>

</core:FragmentDefinition>